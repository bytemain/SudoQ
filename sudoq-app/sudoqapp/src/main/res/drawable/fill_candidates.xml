<?xml version="1.0" encoding="utf-8"?>
<!-- Icon for the Fill Candidates button -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24">
    
    <!-- Grid background representing Sudoku cell -->
    <path
        android:fillColor="#FFFFFF"
        android:pathData="M3,3 L21,3 L21,21 L3,21 Z"
        android:strokeWidth="1"
        android:strokeColor="#FFFFFF"/>
    
    <!-- Small numbers representing candidates in 3x3 grid -->
    <!-- Top row: 1, 2, 3 -->
    <path
        android:fillColor="#888888"
        android:pathData="M5,6 L6,6 L6,8 L5,8 Z"/>
    <path
        android:fillColor="#888888"
        android:pathData="M11,6 L12,6 L12,8 L11,8 Z"/>
    <path
        android:fillColor="#888888"
        android:pathData="M17,6 L18,6 L18,8 L17,8 Z"/>
    
    <!-- Middle row: 4, 5, 6 -->
    <path
        android:fillColor="#888888"
        android:pathData="M5,11 L6,11 L6,13 L5,13 Z"/>
    <path
        android:fillColor="#888888"
        android:pathData="M11,11 L12,11 L12,13 L11,13 Z"/>
    <path
        android:fillColor="#888888"
        android:pathData="M17,11 L18,11 L18,13 L17,13 Z"/>
    
    <!-- Bottom row: 7, 8, 9 -->
    <path
        android:fillColor="#888888"
        android:pathData="M5,16 L6,16 L6,18 L5,18 Z"/>
    <path
        android:fillColor="#888888"
        android:pathData="M11,16 L12,16 L12,18 L11,18 Z"/>
    <path
        android:fillColor="#888888"
        android:pathData="M17,16 L18,16 L18,18 L17,18 Z"/>
</vector>
